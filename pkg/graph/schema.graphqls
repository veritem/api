# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Name {
  first: String!
  middle: String!
  last: String!
  username: String!
}

type Query {
  names: Name!
  status: String!
  blogs: [Blog!]
  socials: [Social!]
}

type Blog {
  link: String!
  dateOfRelease: String!
}

type Social {
  name: String!
  profile: String!
}

type SkillsCategory {
  skill: [Skill!]
}

type Skill {
  name: String!
  description: String!
  createdAt: String!
}
